{
  "name": "@angular/cdk-experimental",
  "version": "20.3.0-next.0",
  "description": "Experimental components for Angular CDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/components.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angular/components/issues"
  },
  "homepage": "https://github.com/angular/components#readme",
  "peerDependencies": {
    "@angular/cdk": "20.3.0-next.0",
    "@angular/core": "^20.0.0-0 || ^20.1.0-0 || ^20.2.0-0 || ^20.3.0-0 || ^21.0.0-0"
  },
  "devDependencies": {
    "@angular/cdk": "workspace:*"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "ng-update": {
    "packageGroup": [
      "@angular/material",
      "@angular/cdk",
      "@angular/cdk-experimental",
      "@angular/material-experimental",
      "@angular/material-luxon-adapter",
      "@angular/material-moment-adapter",
      "@angular/material-date-fns-adapter"
    ]
  },
  "sideEffects": false,
  "module": "./fesm2022/cdk-experimental.mjs",
  "typings": "./index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/cdk-experimental.mjs"
    },
    "./accordion": {
      "types": "./accordion/index.d.ts",
      "default": "./fesm2022/accordion.mjs"
    },
    "./column-resize": {
      "types": "./column-resize/index.d.ts",
      "default": "./fesm2022/column-resize.mjs"
    },
    "./combobox": {
      "types": "./combobox/index.d.ts",
      "default": "./fesm2022/combobox.mjs"
    },
    "./deferred-content": {
      "types": "./deferred-content/index.d.ts",
      "default": "./fesm2022/deferred-content.mjs"
    },
    "./listbox": {
      "types": "./listbox/index.d.ts",
      "default": "./fesm2022/listbox.mjs"
    },
    "./popover-edit": {
      "types": "./popover-edit/index.d.ts",
      "default": "./fesm2022/popover-edit.mjs"
    },
    "./radio-group": {
      "types": "./radio-group/index.d.ts",
      "default": "./fesm2022/radio-group.mjs"
    },
    "./scrolling": {
      "types": "./scrolling/index.d.ts",
      "default": "./fesm2022/scrolling.mjs"
    },
    "./selection": {
      "types": "./selection/index.d.ts",
      "default": "./fesm2022/selection.mjs"
    },
    "./table-scroll-container": {
      "types": "./table-scroll-container/index.d.ts",
      "default": "./fesm2022/table-scroll-container.mjs"
    },
    "./tabs": {
      "types": "./tabs/index.d.ts",
      "default": "./fesm2022/tabs.mjs"
    },
    "./toolbar": {
      "types": "./toolbar/index.d.ts",
      "default": "./fesm2022/toolbar.mjs"
    },
    "./tree": {
      "types": "./tree/index.d.ts",
      "default": "./fesm2022/tree.mjs"
    },
    "./ui-patterns": {
      "types": "./ui-patterns/index.d.ts",
      "default": "./fesm2022/ui-patterns.mjs"
    }
  }
}