    "./combobox": {
      "types": "./types/combobox.d.ts",
      "default": "./fesm2022/combobox.mjs"
    },
    "./deferred-content": {
      "types": "./types/deferred-content.d.ts",
      "default": "./fesm2022/deferred-content.mjs"
    },
    "./listbox": {
      "types": "./types/listbox.d.ts",
      "default": "./fesm2022/listbox.mjs"
    },
    "./popover-edit": {
      "types": "./types/popover-edit.d.ts",
      "default": "./fesm2022/popover-edit.mjs"
