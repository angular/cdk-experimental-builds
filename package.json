    "./radio-group": {
      "types": "./types/radio-group.d.ts",
      "default": "./fesm2022/radio-group.mjs"
    },
    "./scrolling": {
      "types": "./types/scrolling.d.ts",
      "default": "./fesm2022/scrolling.mjs"
    },
    "./selection": {
      "types": "./types/selection.d.ts",
      "default": "./fesm2022/selection.mjs"
    },
    "./table-scroll-container": {
      "types": "./types/table-scroll-container.d.ts",
      "default": "./fesm2022/table-scroll-container.mjs"
    },
    "./tabs": {
      "types": "./types/tabs.d.ts",
      "default": "./fesm2022/tabs.mjs"
    },
    "./toolbar": {
      "types": "./types/toolbar.d.ts",
      "default": "./fesm2022/toolbar.mjs"
    },
    "./tree": {
      "types": "./types/tree.d.ts",
      "default": "./fesm2022/tree.mjs"
    },
    "./ui-patterns": {
      "types": "./types/ui-patterns.d.ts",
      "default": "./fesm2022/ui-patterns.mjs"
    }
  }
}